<%= simple_form_for(user) do |f| %>
  <%= f.error_notification %>
<div class="row">
  <h3>Register Here!</h3>
</div>
<div class="row">
  <div class="form-inputs">
    <div class="col s12 m12 l6">
      <%= f.input :first_name %>
    </div>
    <div class="col s12 m12 l6">
      <%= f.input :last_name %>
    </div>
    <div class="row">
      <div class="col s12 m12 l6">
        <%= f.input :email %>
      </div>
      <div class="col s12 m12 l6">
        <%= f.input :github_username, label: "Github Username" %>
      </div>
    </div>
<div class="row">
  <div class="col s12 m12 l6">
    <%= f.association :school, label: 'Campus', label_method: :city %>
  </div>
</div>

  </div>
</div>
  <div class="row">
    <div class="form-actions">
      <%= f.button :submit, 'Submit' %>
    </div>
  </div>
<% end %>
