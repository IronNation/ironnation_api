</head>

  <body>
      <nav class="tiygray" role="navigation">
        <%= render 'layouts/nav' %>
      </nav>

        <%= yield %>

    <footer class="page-footer teal">
      <%= render 'layouts/footer' %>
    </footer>
  </body>
</html>
